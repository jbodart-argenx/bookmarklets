{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Edge",
            "port": 9222,
            "request": "attach",
            "type": "msedge",
            "webRoot": "${workspaceFolder}/minified",
            "sourceMaps": true,
            "trace": true,
        },
        {
            "name": "Launch localhost:8080 in Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/minified",
            "sourceMaps": true,
            "trace": true,
        },
        {
            "name": "Launch LiveServer localhost:5500 in Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:5500",
            "webRoot": "${workspaceFolder}/minified",
            "sourceMaps": true,
            "trace": true,
        },
        {
            "name": "Launch xartest in Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "https://xartest.ondemand.sas.com/lsaf/",
            "webRoot": "${workspaceFolder}/minified",
            "sourceMaps": true,
            "trace": true,
        },
        {
            "name": "Launch lsafleeds in Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "https://lsafleeds.ondemand.sas.com/lsaf/",
            "webRoot": "${workspaceFolder}/minified",
            "sourceMaps": true,
            "trace": true,
        },
        {
            "name": "Attach to Chrome",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "webRoot": "${workspaceFolder}/minified",
            "sourceMaps": true,
            "trace": true,
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            // "program": "${workspaceFolder}\\beautify.js",
            "program": "${file}"
        }
    ]
}
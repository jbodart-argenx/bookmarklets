{
  "name": "bookmarklets",
  "version": "1.0.0",
  "main": "minify.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "minify": "terser beautified/$npm_config_file.js --source-map url=\"$npm_config_file.min.js.map\" --output minified/$npm_config_file.min.js",
    "minify-win": "terser beautified/%npm_config_file%.js --source-map url=\"%npm_config_file%.min.js.map\" --output minified/%npm_config_file%.min.js",
    "minify-LSAFPathPoll": "npm run minify --file=LSAFPathPoll",
    "minify-win-LSAFPathPoll": "npm run minify-win --file=LSAFPathPoll",
    "minify-app": "npm run minify --file=app",
    "minify-win-app": "npm run minify-win --file=app",
    "launch-chrome": "/System/Volumes/Data/Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome --remote-debugging-port=9222 --user-data-dir=remote-debug-profile",
    "launch-edge": "/System/Volumes/Data/Applications/Microsoft\\ Edge.app/Contents/MacOS/Microsoft\\ Edge --remote-debugging-port=9222 --user-data-dir=remote-debug-profile"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "uglify-js": "^3.19.2"
  },
  "devDependencies": {
    "terser": "^5.31.6"
  }
}
